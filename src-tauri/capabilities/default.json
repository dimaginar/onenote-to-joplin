{
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:allow-save",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        { "url": "https://joplinapp.org/*" },
        { "url": "https://github.com/alxnbl/onenote-md-exporter*" }
      ]
    }
  ]
}
